{
  "root": "apps/vroomdle",
  "sourceRoot": "apps/vroomdle/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/vroomdle",
        "commands": [
          {
            "command": "vite build --outDir ../../dist/vroomdle/ --emptyOutDir"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/vroomdle",
        "command": "vite preview"
      }
    },
    "start": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/vroomdle",
        "command": "vite"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/vroomdle"
      ],
      "options": {
        "jestConfig": "apps/vroomdle/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/vroomdle/src/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
{
  "root": "apps/cashconverters",
  "sourceRoot": "apps/cashconverters/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx-plugin-esbuild:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/cashconverters/dist",
        "main": "apps/cashconverters/src/main.ts",
        "tsConfig": "apps/cashconverters/tsconfig.app.json",
        "assets": ["apps/cashconverters/src/assets"],
        "tsconfigPath": "apps/cashconverters/tsconfig.json",
        "watch": false,
        "bundle": true,
        "platform": "node",
        "extendConfig": "nx-esbuild.ts"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/cashconverters/src/environments/environment.ts",
              "with": "apps/cashconverters/src/environments/environment.prod.ts"
            }
          ],
          "skipTypeCheck": false,
          "sourceMap": "both",
          "bundle": true,
          "externalDependencies": "all",
          "minify": true,
          "extractLicenses": true
        }
      }
    },
    "serve": {
      "executor": "nx-plugin-workspace:node-serve",
      "options": {
        "buildTarget": "cashconverters:esbuild-build"
      },
      "configurations": {
        "production": {
          "buildTarget": "cashconverters:esbuild-build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/cashconverters/**/*.ts"]
      }
    }
  },
  "tags": []
}
